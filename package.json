{
  "name": "gethhmmss",
  "version": "2.0.0",
  "description": "Convert seconds or Data object to \"hh:mm:ss\" format.",
  "main": "dist/gethhmmss.cjs.js",
  "module": "dist/gethhmmss.esm.js",
  "browser": "dist/gethhmmss.umd.js",
  "scripts": {
    "i": "npm install",
    "build": "rollup -c",
    "test": "ava",
    "deploy": "scripts/deploy",
    "gadd": "git add -u; echo >/dev/null"
  },
  "pre-commit": [
    "i",
    "test",
    "build",
    "gadd"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/examus/gethhmmss.git"
  },
  "keywords": [
    "hhmmss",
    "date",
    "time",
    "seconds",
    "format"
  ],
  "author": "Examus team",
  "ava": {
    "require": [
      "babel-register"
    ]
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/examus/gethhmmss/issues"
  },
  "homepage": "https://github.com/examus/gethhmmss#readme",
  "devDependencies": {
    "ava": "^2.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "pre-commit": "^1.2.2",
    "rollup": "^1.17.0"
  }
}
